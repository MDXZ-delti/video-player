{
	"manifest_version": 3,
	"name": "Player for local videos",
	"version": "0.2",
	"author": "Matteo Delton",
	"description": "Player for local videos with keyboard shortcuts.",
	"offline_enabled": true,
	"action": { },
	"background": {
		"service_worker": "js/background.js"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+O"
			}
		}
	},
	"icons": [
		{
			"src": "favicon.png",
			"sizes": "192x192"
		}
	]
}
